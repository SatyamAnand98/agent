{
    "name": "local-code-agent",
    "displayName": "Local Code Agent",
    "version": "0.0.1",
    "publisher": "you",
    "engines": {
        "vscode": "^1.90.0"
    },
    "activationEvents": [
        "onCommand:agent.analyze",
        "onCommand:agent.apply"
    ],
    "contributes": {
        "commands": [
            {
                "command": "agent.analyze",
                "title": "Agent: Analyze prompt.txt"
            },
            {
                "command": "agent.apply",
                "title": "Agent: Apply Plan (dry-run)"
            }
        ]
    },
    "main": "./out/extension.js",
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -p ./"
    },
    "devDependencies": {
        "typescript": "^5.5.4",
        "@types/vscode": "^1.90.0"
    }
}